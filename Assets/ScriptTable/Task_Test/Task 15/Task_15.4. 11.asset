    Text_Learnihg_for_Block: "15.4.11. \u041F\u0440\u043E\u0432\u0435\u0440\u044C\u0442\u0435
      \u043F\u0440\u0430\u0432\u0438\u043B\u044C\u043D\u043E\u0441\u0442\u044C \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0438
      \u0410\u041F\u0427 \u043D\u0430 3 \u0432\u043E\u043B\u043D\u0435, \u0434\u043B\u044F
      \u0447\u0435\u0433\u043E:\r\n-   \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435
      \u043D\u0430 \u043F\u0443\u043B\u044C\u0442\u0435 \u041F\u041E\u0421-72 \u043F\u0435\u0440\u0435\u043A\u043B\u044E\u0447\u0430\u0442\u0435\u043B\u044C
      \u0420\u0415\u0416\u0418\u041C \u0420\u0410\u0411\u041E\u0422\u042B \u0432
      \u043F\u043E\u043B\u043E\u0436\u0435-\u043D\u0438\u0435 \u0410;\r\n-  \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435
      \u043F\u0435\u0440\u0435\u043A\u043B\u044E\u0447\u0430\u0442\u0435\u043B\u044C 
      \u0412\u0420\u0410\u0429\u0415\u041D\u0418\u0415-\u0414\u041E\u0412\u041E\u0420\u041E\u0422 
      \u0432  \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0414\u041E\u0412\u041E-\u0420\u041E\u0422;\r\n- 
      \u043D\u0430\u043F\u0440\u0430\u0432\u044C\u0442\u0435 \u0430\u043D\u0442\u0435\u043D\u043D\u0443
      \u043D\u0430 \xAB\u043C\u0435\u0441\u0442\u043D\u044B\u0435 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u044B\xBB,
      \u043C\u0430\u043D\u0438\u043F\u0443\u043B\u0438\u0440\u0443\u044F \u043F\u0435\u0440\u0435\u043A\u043B\u044E\xAC\u0447\u0430\u0442\u0435\u043B\u0435\u043C
      \u0412\u041B\u0415\u0412\u041E-\u0412\u041F\u0420\u0410\u0412\u041E, \u0441
      \u043E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0439 \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043A\u043E\u0439
      \u0432 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u041E\u0422\u041A\u041B.,
      \u043F\u0440\u0438 \u044D\u0442\u043E\u043C \u043D\u0430 \u044D\u043A\u0440\u0430\u043D\u0435
      \u043E\u0441\u0446\u0438\u043B\u043B\u043E\u0433\u0440\u0430\u0444\u0430 \u0431\u043B\u043E\u043A\u0430
      O-71 \u043F\u0440\u043E\u0441\u043C\u0430\u0442\u0440\u0438\u0432\u0430\u044E\u0442\u0441\u044F
      \u044D\u0445\u043E-\u0441\u0438\u0433\u043D\u0430\u043B\u044B \xAB\u043C\u0435\u0441\u0442\u043D\u044B\u0445
      \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432\xBB;\r\n\u0412\u043D\u0438\u043C\u0430\u043D\u0438\u0435!
      \u0412\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u0435 \u043F\u0435\u0440\u0435\u043A\u043B\u044E\u0447\u0430\u0442\u0435\u043B\u044F
      \u0434\u043E\u0432\u043E\u0440\u043E\u0442 \u043D\u0430 \u0432\u0440\u0435\u043C\u044F
      \u0441\u0432\u044B\u0448\u0435 30 \u0441 \u043D\u0435 \u0434\u043E\u043F\u0443\u0441-\u043A\u0430\u0435\u0442\u0441\u044F.\r\n- 
      \u0434\u043E\u0431\u0435\u0439\u0442\u0435\u0441\u044C \u0440\u0443\u0447\u043A\u0430\u043C\u0438
      \u0423\u0421\u0418\u041B\u0415\u041D\u0418\u0415 \u0431\u043B\u043E\u043A\u043E\u0432
      O-71 \u0438 \u0415-71, \u0447\u0442\u043E\u0431\u044B \u044D\u0445\u043E-\u0441\u0438\u0433\u043D\u0430\u043B\u044B
      \u043C\u0435\u0441\u0442\u043D\u044B\u0445 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432
      \u043D\u0435 \u043E\u0433\u0440\u0430\u043D\u0438\u0447\u0438\u0432\u0430\u043B\u0438\u0441\u044C;\r\n- 
      \u0443\u0431\u0435\u0434\u0438\u0442\u0435\u0441\u044C, \u0432\u0440\u0430\u0449\u0430\u044F
      \u0440\u0443\u0447\u043A\u0443 \u0420\u0415\u0413\u0423\u041B\u0418\u0420.
      \u0427\u0410\u0421\u0422\u041E\u0422\u042B \u0431\u043B\u043E\u043A\u0430 \u0413-71
      \u0432 \u043D\u0435\xAC\u0431\u043E\u043B\u044C\u0448\u0438\u0445 \u043F\u0440\u0435\u0434\u0435\u043B\u0430\u0445
      \u0432 \u043E\u0434\u043D\u0443 \u0438 \u0432 \u0434\u0440\u0443\u0433\u0443\u044E
      \u0441\u0442\u043E\u0440\u043E\u043D\u0443, \u0432 \u0441\u0438\u043C\u043C\u0435\u0442\u0440\u0438\u0447\u043D\u043E\u043C
      \u0443\u043C\u0435\u043D\u044C\u0448\u0435\u043D\u0438\u0438 \u0430\u043C\u043F\u043B\u0438\u0442\u0443-\u0434\u044B
      \u044D\u0445\u043E-\u0441\u0438\u0433\u043D\u0430\u043B\u043E\u0432 \xAB\u043C\u0435\u0441\u0442\u043D\u044B\u0445
      \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432\xBB \u043D\u0430 \u044D\u043A\u0440\u0430\u043D\u0435
      \u043E\u0441\u0446\u0438\u043B\u043B\u043E\u0433\u0440\u0430\u0444\u0430 0-71.
      \u041F\u0440\u0438 \u043E\u0442-\u043F\u0443\u0441\u043A\u0430\u043D\u0438\u0438
      \u0440\u0443\u0447\u043A\u0438 \u043E\u043D\u0430 \u0434\u043E\u043B\u0436\u043D\u0430
      \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0442\u044C\u0441\u044F \u0432
      \u0440\u0430\u0431\u043E\u0447\u0435\u0435 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435.
      \u0415\u0441\u043B\u0438 \u043C\u0430\u043A\u0441\u0438-\u043C\u0443\u043C
      \u0430\u043C\u043F\u043B\u0438\u0442\u0443\u0434\u044B \u043C\u0435\u0441\u0442\u043D\u044B\u0445
      \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432 \u043D\u0435 \u0441\u043E\u0432\u043F\u0430\u0434\u0430\u0435\u0442
      \u0441 \u0440\u0430\u0431\u043E\u0447\u0438\u043C \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\xAC\u0435\u043C
      \u0440\u0443\u0447-\u043A\u0438 \u0420\u0415\u0413\u0423\u041B\u0418\u0420.
      \u0427\u0410\u0421\u0422\u041E\u0422\u042B, \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C\u043E
      \u0434\u043E\u0431\u0438\u0442\u044C\u0441\u044F \u0438\u0445 \u0441\u043E\u0432\u043F\u0430\u0434\u0435\u043D\u0438\u044F
      \u0432\u0440\u0430\xAC\u0449\u0435\u043D\u0438\u0435\u043C \u0448\u043B\u0438\u0446\u0430
      \u041D\u0423\u041B\u042C \u0414\u0418\u0421\u041A\u0420\u0418\u041C.  \u043D\u0430
      \u0433\u043E\u0440\u0438\u0437\u043E\u043D\u0442\u0430\u043B\u044C\u043D\u043E\u0439
      \u043F\u0430\u043D\u0435\u043B\u0438 \u0441\u0443\u0431\u0431\u043B\u043E\u043A\u0430
      \u0415\u0410-71;\r\n-  \u043F\u0440\u043E\u0438\u0437\u0432\u0435\u0441\u0442\u0438
      \u0430\u043D\u0430\u043B\u043E\u0433\u0438\u0447\u043D\u0443\u044E \u043F\u0440\u043E\u0432\u0435\u0440\u043A\u0443
      \u043D\u0430 \u043E\u0441\u0442\u0430\u043B\u044C\u043D\u044B\u0445 \u0432\u043E\u043B\u043D\u0430\u0445.
      \u041D\u0430 \u0432\u043E\u043B\u043D\u0435 2 \u0441\u043E\u0432\u043C\u0435-\u0449\u0435\u043D\u0438\u0435
      \u043C\u0430\u043A\u0441\u0438\u043C\u0443\u043C\u0430 \u0430\u043C\u043F\u043B\u0438\u0442\u0443\u0434\u044B
      \u043C\u0435\u0441\u0442\u043D\u044B\u0445 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u0432
      \u0441 \u0440\u0430\u0431\u043E\u0447\u0438\u043C \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\xAC\u0435\u043C
      \u0440\u0443\u0447\u043A\u0438 \u0420\u0415\u0413\u0423\u041B\u0418\u0420.
      \u0427\u0410\u0421\u0422\u041E\u0422\u042B \u043E\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043B\u044F\u0435\u0442\u0441\u044F
      \u0448\u043B\u0438\u0446\u043E\u043C \u0411\u0410\u041B\u0410\u041D\u0421,
      \u0440\u0430\u0441\u043F\u043E-\u043B\u043E\u0436\u0435\u043D\u043D\u044B\u043C
      \u043D\u0430 \u0433\u043E\u0440\u0438\u0437\u043E\u043D\u0442\u0430\u043B\u044C\u043D\u043E\u0439
      \u043F\u0430\u043D\u0435\u043B\u0438 \u0441\u0443\u0431\u0431\u043B\u043E\u043A\u0430
      \u0415\u0410-71.\r\n\u041F\u0440\u0438\u043C\u0435\u0447\u0430\u043D\u0438\u0435.
      \u041F\u0440\u043E\u0432\u0435\u0440\u043A\u0430 \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0438
      \u0441\u0438\u0441\u0442\u0435\u043C\u044B \u0410\u041F\u0427 \u043C\u043E\u0436\u0435\u0442
      \u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0438\u0442\u044C\u0441\u044F
      \u043F\u043E \u0437\u043E\u043D\u0434\u0438\u0440\u0443\u044E\u0449\u0435\u043C\u0443
      \u0438\u043C\u043F\u0443\u043B\u044C\u0441\u0443 \u043F\u0435\u0440\u0435\u0434\u0430\u0442\u0447\u0438\u043A\u0430,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u043D\u0430\u0431\u043B\u044E\u0434\u0430\u0435\u0442\u0441\u044F
      \u043D\u0430 \u044D\u043A\u0440\u0430\u043D\u0435 \u043E\u0441-\u0446\u0438\u043B\u043B\u043E\u0433\u0440\u0430\u0444\u0430
      \u0431\u043B\u043E\u043A\u0430 0-71 \u043F\u0440\u0438 \u043C\u0430\u0441\u0448\u0442\u0430\u0431\u0435
      \u0440\u0430\u0437\u0432\u0435\u0440\u0442\u043A\u0438 02. \u043C\u0435\u0442\u043E\u0434\u0438\u043A\u0430
      \u043F\u0440\u043E\u0432\u0435\u0440\u043A\u0438 \u043E\u0441-\u0442\u0430\u0435\u0442\u0441\u044F
      \u043F\u0440\u0435\u0436\u043D\u0435\u0439.\r"
